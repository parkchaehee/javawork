package singleton.student;

public class StudentMain {

	public static void main(String[] args) {

		School sch = School.getInstance();
				
		Student stu1 = sch.createStudent();
		Student stu2 = sch.createStudent();
		
		System.out.println("학번: " + stu1.getStudentNum());
		System.out.println("학번: " + stu2.getStudentNum());

	}

}
