package inheritance.statement;

public class SaileStatement {

	public static void main(String[] args) {
		//Drink 클래스 사용
		Drink coffee = new Drink("커피", 2500, 10);
		Drink 녹차 = new Drink("녹차", 3000, 4);
		
		//전표출력
		Drink.printTitle();
		coffee.printData(); //객체이름.출력
		녹차.printData(); //객체이름.출력
		

	}

}
