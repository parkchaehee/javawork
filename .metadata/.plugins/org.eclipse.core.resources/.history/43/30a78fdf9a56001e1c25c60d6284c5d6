package generic;

import java.util.ArrayList;

public class GenericTest {

	public static void main(String[] args) {
		// Generic(일반화) 이전의 코드 작성
		ArrayList cartlist = new ArrayList();
		cartlist.add("egg");
		cartlist.add("coffee");
		
		String item = (String)cartlist.get(0);
		System.out.println(item);
		
		
		//Generic을 이용한 코드 작성

	}

}
