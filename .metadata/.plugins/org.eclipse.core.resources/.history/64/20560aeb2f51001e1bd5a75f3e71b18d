package statics;

public class Student {
	static int serialNum = 1000;//클래스 변수(필드) //static=>누적//static없으면 둘다1001나옴
	int id;      //인스턴스 변수(필드)
	String name; //인스턴스 변수(필드)
	
	/*public Student() {
		serialNum++;
		id = serialNum;
	}*/
	//입력
	public void setId(int id) {
		this.id = id;
	}
	//출력
	public int getId(int id) {
		return id;
	}
}
