package objectequals.memeber;

public class Member {
	private String memberId; //사번
	private String name;     //이름
	
	public Member(String memberId, String name) {
		this.memberId = memberId;
		this.name = name;
	}
	//hashCode() 재정의
	@Override
	public int hashCode() { //멤버이름.hashCode()를 반환
		return memberId.hashCode();
	}
	//equals()메서드 재정의
	@Override
	public boolean equals(Object obj) {
		if(obj instanceof Member) {
			Member member = (Member)obj; //Member형으로 다운캐스팅
			if(this.memberId.equals(member.memberId)) {
				return true;//외부입력된 회원아이디와 이미생성된 회원아이디가 일치하면true
			}
		}
		return false;
	}


}
