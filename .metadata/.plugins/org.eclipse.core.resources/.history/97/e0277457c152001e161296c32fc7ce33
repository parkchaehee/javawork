package kr.or.iei.model.vo;

public class User {

	private String id;

	private String pwd;

	private String name;

	private int age;

	private int phone; //String
	
	public User() {}

	public User(String id, String pwd, String name, int age, int phone) {

	}
	
	public void setId(String id) {

		this.id = id;

	}

	public String getId() {

		return id;

	}

	public void setPwd(String pwd) {

		this.pwd = pwd;

}

	public String getPwd() {

		return pwd;

}

	public void setName(String name) {

		this.name = name;

}

	public String getName() {

		return name;

}

	public void setAge(int age) {

		this.age = age;

}

	public int getAge() {

		return age;

}



	public int getPhone() {

		return phone;

}



	@Override

	public String toString() {

		return "id : " + id + ", pwd : " + pwd + ", name : " + name + ", age : " + age + ", phone : " + phone;

}

}