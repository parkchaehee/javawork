package objecaaray.dogs;

import Books.Book;

public class DogArray {

	public static void main(String[] args) {
		// Dog 인스턴스 4개를 생성해서 배열에 저장하고
		// 정보를 출력하세요(get()함수 사용)
		Dog[] name = new Dog[4];
	
		name[0] = new Dog("백구","진돗개");
		name[1] = new Dog("백구1","진돗개1");
		name[2] = new Dog("백구2","진돗개2");
		name[3] = new Dog("백구3","진돗개3");
		
		//name[2].showDogInfo();
		
		System.out.println();
		
		for(int i=0; i<name.length; i++) {
			/*library[i].showBookInfo();*/
			System.out.println(name[i].showDogInfo());
		}		
		
		//향상된 for 문
		//for(자료형 변수 : 배열){}
		for(Dog dog : name)
			System.out.println(dog.showDogInfo());

		System.out.println();



	}

}
